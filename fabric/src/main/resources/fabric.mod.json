{
  "schemaVersion": 1,
  "id": "simplewhitelist",
  "version": "${mod_version}",
  "name": "SimpleWhitelist",
  "description": "Simple whitelist system for your server",
  "authors": [
    "An1by"
  ],
  "contact": {
    "sources": "https://github.com/an1by/SimpleWhitelist",
    "issues": "https://github.com/an1by/SimpleWhitelist/issues",
    "homepage": "https://modrinth.com/plugin/simplewhitelist"
  },
  "license": "All Rights Reserved",
  "icon": "assets/simplewhitelist/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      "net.aniby.simplewhitelist.fabric.FabricWhitelistMod"
    ]
  },
  "depends": {
    "fabricloader": "${fabric_loader_dependency}",
    "fabric": "*",
    "minecraft": [
      "1.21",
      "1.21.1"
    ],
    "java": ">=21"
  },
  "breaks": {
    "fabric-api": "${fabric_api_dependency_breaks}"
  },
  "suggests": {
    "fabric-permissions-api-v0": "*"
  }
}
